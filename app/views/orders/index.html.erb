<div class="info">
    <h2 class="no-margin"><a href="detail.html"><%= t ".title" %></a></h2>
    <hr>
</div>
<!--end info-->
<div class="col-md-3"></div>

<div class="sidebar">
    <div class="col-md-9 box">
        <table class="table table-consender">
            <thead class="header">
            <tr>
                <th>ID</th>
                <th><%= t ".place_name" %></th>
                <th><%= t ".receive_address" %></th>
                <th><%= t ".distance" %></th>
                <th><%= t ".ship_price" %></th>
                <th><%= t ".total_price" %></th>
                <th><%= t ".status" %></th>
                <th><%= t ".payment_status" %></th>
                <th><%= t ".action" %></th>
            </tr>
            </thead>
            <tbody>
              <%= render @orders %>
            </tbody>
        </table>
        <!--end reservations-->
        <div class="center">
            <%= will_paginate @orders %>
        </div>
        <!-- end center-->
    </div>
</div>
