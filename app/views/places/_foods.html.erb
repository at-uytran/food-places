    <section id="availability" class="box">
        <h2><%= t ".food_menu" %></h2>
        <div class="form-foods">
            <div class="table-responsive pricing-table">
                <table class="table">
                    <thead>
                        <tr>
                            <th class="width-30">
                                <span class="title"><%= t ".food_name" %></span>
                            </th>
                            <th class="section">
                                <span class="title"><%= t ".food_category" %></span>
                            </th>
                            <th class="section">
                                <span class="title"><%= t ".price" %></span>
                            </th>
                            <th class="section">
                                <span class="title"><%= t ".order" %></span>
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <%= render partial: "food", collection: @foods, as: :food %>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="center">
            <a href="" class="btn btn-default btn-rounded btn-framed">Load More</a>
        </div>
        <!--end form-reservations-->
    </section>
